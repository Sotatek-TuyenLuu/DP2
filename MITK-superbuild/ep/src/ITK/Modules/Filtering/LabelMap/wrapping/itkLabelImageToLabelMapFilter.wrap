itk_wrap_class("itk::LabelImageToLabelMapFilter" POINTER)
  foreach(t ${WRAP_ITK_USIGN_INT})
    foreach(d ${ITK_WRAP_DIMS})
      # image -> label collection image
      itk_wrap_template("${ITKM_I${t}${d}}${ITKM_LM${d}}" "${ITKT_I${t}${d}}, ${ITKT_LM${d}}")
    endforeach(d)
  endforeach(t)
itk_end_wrap_class()
