itk_wrap_class("itk::BayesianClassifierImageFilter" POINTER)
  foreach(d ${ITK_WRAP_DIMS})
    foreach(t ${WRAP_ITK_SCALAR})
      foreach(t2 ${WRAP_ITK_INT})
        foreach(t3 ${WRAP_ITK_REAL})
          itk_wrap_template("${ITKM_VI${t}${d}}${ITKM_${t2}}${ITKM_${t3}}${ITKM_${t3}}" "${ITKT_VI${t}${d}}, ${ITKT_${t2}}, ${ITKT_${t3}}, ${ITKT_${t3}}")
        endforeach(t3)
      endforeach(t2)
    endforeach(t)
  endforeach(d)
itk_end_wrap_class()
